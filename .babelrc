{
	"presets": [
		"flow",
		"es2015",
		"stage-0"
	],
	"plugins": [
		"add-module-exports",
		"transform-runtime",
		"syntax-decorators", [
			"import", {
				"libraryName": "antd",
				"libraryDirectory": "lib",
				"style": true,
			}
		]
	],
	"env": {
		"test": {
			"plugins": ["istanbul"]
		}
	}
}
